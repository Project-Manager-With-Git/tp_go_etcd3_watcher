{
    "language": "go",
    "env": "gomod",
    "description": "etcd的监控程序模板",
    "author": "hsz",
    "template_type": "watcher",
    "requires": [
        "github.com/Golang-Tools/loggerhelper@v0.0.3",
        "github.com/Golang-Tools/schema-entry-go@v0.0.4",
        "go.etcd.io/etcd/api/v3@v3.5.0",
        "go.etcd.io/etcd/client/v3@v3.5.0"
    ],
    "template_keys": {
        "app_name": {
            "description": "项目名",
            "default": "{{ project_name }}"
        }
    },
    "components": {
        "etcdproxy": {
            "source": "etcdproxy",
            "description": "etcd的代理模板",
            "default_path": "etcdproxy"
        },
        "watcher": {
            "source": "watcher",
            "description": "监听器模板",
            "default_path": "watcher"
        },
        "sender": {
            "source": "sender",
            "description": "写入端模板",
            "default_path": "sender"
        },
        "main": {
            "source": "main.go.jinja",
            "description": "",
            "default_path": "{{ project_name }}.go"
        },
        "dockerfile": {
            "source": "dockerfile.jinja",
            "description": "构造镜像用的脚本",
            "default_path": "dockerfile"
        },
        "dockercompose": {
            "source": "cp_dockercompose@v0.0.2//etcd_broker_standalone",
            "description": "用于测试程序的docker-compose文件",
            "default_path": "docker-compose.yml"
        }
    }
}